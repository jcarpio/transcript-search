{
  "version": 2,
  "builds": [
    {
      "src": "server/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/load-data",
      "dest": "server/app.js"
    },
    {
      "src": "/health",
      "dest": "server/app.js"
    },
    {
      "src": "/search",
      "dest": "server/app.js"
    },
    {
      "src": "/paragraphs",
      "dest": "server/app.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/search.html",
      "dest": "/public/search.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
